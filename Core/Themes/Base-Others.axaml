<ResourceDictionary xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:sys="using:System">

	<!-- Animation -->
	<Easing x:Key="Easing/Animation">ExponentialEaseOut</Easing>
	<sys:TimeSpan x:Key="TimeSpan/Animation">0:0:0.5</sys:TimeSpan>
	<sys:TimeSpan x:Key="TimeSpan/Animation.Fast">0:0:0.3</sys:TimeSpan>
	<sys:TimeSpan x:Key="TimeSpan/Animation.Slow">0:0:0.8</sys:TimeSpan>

	<!-- ApplicationInfoDialog -->
	<sys:Double x:Key="Double/ApplicationInfoDialog.AppVersion.Opacity">0.6</sys:Double>

	<!-- FontFamilies -->
	<FontFamily x:Key="FontFamily/DefaultSystemFixWidth">Consolas,Courier New</FontFamily>

	<!-- Icon -->
	<sys:Double x:Key="Double/Icon.Opacity.Disabled">0.3</sys:Double>
	
	<!-- ItemsPanelTemplate -->
	<ItemsPanelTemplate x:Key="ItemsPanelTemplate/StackPanel">
		<StackPanel/>
	</ItemsPanelTemplate>

	<!-- MainWindow -->
	<sys:TimeSpan x:Key="TimeSpan/MainWindow.ContentPaddingTransition">0:0:0.5</sys:TimeSpan>
	
	<!-- NotificationPresenter -->
	<StaticResourceExtension x:Key="TimeSpan/NotificationPresenter.Notification" ResourceKey="TimeSpan/Animation"/>

	<!-- TextBox -->
	<sys:Double x:Key="Double/TextBox.Assistance.MenuItem.Description.Opacity">0.5</sys:Double>

</ResourceDictionary>
