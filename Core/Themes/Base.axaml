	<Style Selector="ScrollBar:horizontal">
		<Setter Property="Template">
			<ControlTemplate>
				<Grid x:Name="Root">
					<Border x:Name="HorizontalRoot"
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}">
						<Grid ColumnDefinitions="Auto,*,Auto">
							<Border Name="TrackBorder" CornerRadius="{TemplateBinding CornerRadius}" Grid.ColumnSpan="3" Margin="0" />
							<RepeatButton Name="PART_LineUpButton"
											VerticalAlignment="Center"
											Classes="line up"
											Grid.Column="0"
											Focusable="False"
											MinHeight="{DynamicResource ScrollBarSize}"/>
							<Track Grid.Column="1"
									Minimum="{TemplateBinding Minimum}"
									Maximum="{TemplateBinding Maximum}"
									Value="{TemplateBinding Value, Mode=TwoWay}"
									ViewportSize="{TemplateBinding ViewportSize}"
									Orientation="{TemplateBinding Orientation}">
								<Track.DecreaseButton>
								<RepeatButton Name="PART_PageUpButton"
												Classes="largeIncrease"
												Focusable="False" />
								</Track.DecreaseButton>
								<Track.IncreaseButton>
								<RepeatButton Name="PART_PageDownButton"
												Classes="largeIncrease"
												Focusable="False" />
								</Track.IncreaseButton>
								<Thumb Classes="thumb"
									Opacity="1"
									Height="{StaticResource Double/ScrollBar.Thumb.Size}"
									MinWidth="{StaticResource ScrollBarSize}"
									VerticalAlignment="Center" />
							</Track>
							<RepeatButton Name="PART_LineDownButton"
											VerticalAlignment="Center"
											Classes="line down"
											Grid.Column="2"
											Focusable="False"
											MinHeight="{StaticResource ScrollBarSize}"/>
						</Grid>
					</Border>
				</Grid>
			</ControlTemplate>
		</Setter>
	</Style>
	<Style Selector="ScrollBar:vertical">
		<Setter Property="Template">
			<ControlTemplate>
				<Grid x:Name="Root">
					<Border x:Name="VerticalRoot"
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}">
						<Grid RowDefinitions="Auto,*,Auto">
							<Border Name="TrackBorder" CornerRadius="{TemplateBinding CornerRadius}" Grid.RowSpan="3" Margin="0" />
							<RepeatButton Name="PART_LineUpButton"
											HorizontalAlignment="Stretch"
											Classes="line up"
											Grid.Row="0"
											Focusable="False"/>
							<Track Grid.Row="1"
									Minimum="{TemplateBinding Minimum}"
									Maximum="{TemplateBinding Maximum}"
									Value="{TemplateBinding Value, Mode=TwoWay}"
									ViewportSize="{TemplateBinding ViewportSize}"
									Orientation="{TemplateBinding Orientation}"
