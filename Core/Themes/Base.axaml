<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:sys="using:System"
		xmlns:abControls="using:CarinaStudio.Controls"
		xmlns:abConverters="using:CarinaStudio.Data.Converters"
		xmlns:aConverters="using:Avalonia.Data.Converters"
		xmlns:appSuite="using:CarinaStudio.AppSuite"
		xmlns:controls="using:CarinaStudio.AppSuite.Controls"
		xmlns:converters="using:CarinaStudio.AppSuite.Converters">

	<!-- Resources -->
	<Styles.Resources>

		<!-- Accent -->
		<Color x:Key="Color/Accent">#912fbf</Color>
		<SolidColorBrush x:Key="Brush/Accent" Color="{DynamicResource Color/Accent}"/>
		<SolidColorBrush x:Key="Brush/Accent.Dark.1" Color="{DynamicResource SystemAccentColorDark1}"/>
		<SolidColorBrush x:Key="Brush/Accent.Dark.2" Color="{DynamicResource SystemAccentColorDark2}"/>
		<SolidColorBrush x:Key="Brush/Accent.Dark.3" Color="{DynamicResource SystemAccentColorDark3}"/>
		<SolidColorBrush x:Key="Brush/Accent.Light.1" Color="{DynamicResource SystemAccentColorLight1}"/>
		<SolidColorBrush x:Key="Brush/Accent.Light.2" Color="{DynamicResource SystemAccentColorLight2}"/>
		<SolidColorBrush x:Key="Brush/Accent.Light.3" Color="{DynamicResource SystemAccentColorLight3}"/>

		<!-- AddTabItemButton -->
		<sys:Double x:Key="Double/AddTabItemButton.Size">32</sys:Double>
		<Thickness x:Key="Thickness/AddTabItemButton.Padding">7</Thickness>

		<!-- Animation -->
		<Easing x:Key="Easing/Animation">ExponentialEaseOut</Easing>
		<sys:TimeSpan x:Key="TimeSpan/Animation">0:0:0.5</sys:TimeSpan>
		<sys:TimeSpan x:Key="TimeSpan/Animation.Fast">0:0:0.2</sys:TimeSpan>
		<sys:TimeSpan x:Key="TimeSpan/Animation.Slow">0:0:0.8</sys:TimeSpan>

		<!-- ApplicationChangeListDialog -->
		<sys:Double x:Key="Double/ApplicationChangeListDialog.Content.MaxHeight">500</sys:Double>
		<sys:Double x:Key="Double/ApplicationChangeListDialog.Width">500</sys:Double>
		<Thickness x:Key="Thickness/ApplicationChangeListDialog.Change.Description.Margin">0,-1,0,0</Thickness>

		<!-- ApplicationInfoDialog -->
		<sys:Double x:Key="Double/ApplicationInfoDialog.AppBadge.Size">20</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.AppIcon.Size">60</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.AppTitle.FontSize">25</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.AppVersion.Opacity">0.5</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.Content.MaxHeight">500</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.Header.Background.Height">120</sys:Double>
		<sys:Double x:Key="Double/ApplicationInfoDialog.MinWidth">500</sys:Double>
		<Thickness x:Key="Thickness/ApplicationInfoDialog.AppBadge.Margin">5,0,0,0</Thickness>
		<Thickness x:Key="Thickness/ApplicationInfoDialog.AppIcon.Margin">0,0,15,0</Thickness>
		<Thickness x:Key="Thickness/ApplicationInfoDialog.AppTitle.Margin">0,0,0,5</Thickness>

		<!-- ApplicationOptionsDialog -->
		<sys:Double x:Key="Double/ApplicationOptionsDialog.Content.MaxHeight">500</sys:Double>
		<sys:Double x:Key="Double/ApplicationOptionsDialog.Width">500</sys:Double>

		<!-- ApplicationUpdateDialog -->
		<sys:Double x:Key="Double/ApplicationUpdateDialog.Width">500</sys:Double>

		<!-- BrowseButton -->
		<sys:Double x:Key="Double/BrowseButton.Icon.Size">14</sys:Double>
		<Thickness x:Key="Thickness/BrowseButton.Margin">5,0,0,0</Thickness>
		<Thickness x:Key="Thickness/BrowseButton.Padding">8</Thickness>

		<!-- Button -->
		<SolidColorBrush x:Key="AccentButtonBackground" Color="{DynamicResource SystemAccentColor}" />
		<SolidColorBrush x:Key="AccentButtonBackgroundPointerOver" Color="{DynamicResource SystemAccentColorLight1}" />
		<SolidColorBrush x:Key="AccentButtonBackgroundPressed" Color="{DynamicResource SystemAccentColorDark1}"/>
		<SolidColorBrush x:Key="ButtonBackground" Color="{DynamicResource Color/Button.Background}"/>
		<SolidColorBrush x:Key="ButtonBackgroundDisabled" Color="{DynamicResource Color/Button.Background.Disabled}"/>
		<SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="{DynamicResource Color/Button.Background.PointerOver}"/>
		<SolidColorBrush x:Key="ButtonBackgroundPressed" Color="{DynamicResource Color/Button.Background.Pressed}"/>
		<LinearGradientBrush x:Key="ButtonBorderBrush" EndPoint="0%,100%" StartPoint="0%,0%">
			<GradientStop Offset="0" Color="{DynamicResource Color/Control.Border.Light}"/>
			<GradientStop Offset="1" Color="{DynamicResource Color/Control.Border.Dark}"/>
		</LinearGradientBrush>
		<StaticResourceExtension x:Key="ButtonBorderBrushDisabled" ResourceKey="ButtonBorderBrush"/>
		<StaticResourceExtension x:Key="ButtonBorderBrushPointerOver" ResourceKey="ButtonBorderBrush"/>
		<LinearGradientBrush x:Key="ButtonBorderBrushPressed" EndPoint="0%,100%" StartPoint="0%,0%">
			<GradientStop Offset="0" Color="{DynamicResource Color/Control.Border.Dark}"/>
			<GradientStop Offset="1" Color="{DynamicResource Color/Control.Border.Light}"/>
		</LinearGradientBrush>
		<Color x:Key="Color/Button.Background">#ffffff</Color>
		<Color x:Key="Color/Button.Background.Disabled">#ffffff</Color>
		<Color x:Key="Color/Button.Background.PointerOver">#ffffff</Color>
		<Color x:Key="Color/Button.Background.Pressed">#ffffff</Color>
		<Thickness x:Key="Thickness/Button.Border">1</Thickness>

		<!-- CheckBox -->
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundStrokeIndeterminatePointerOver" Color="{DynamicResource SystemAccentColorLight1}"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundStrokeIndeterminatePressed" Color="{DynamicResource SystemAccentColorDark1}"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundStrokeCheckedPointerOver" Color="{DynamicResource SystemAccentColorLight1}"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundStrokeCheckedPressed" Color="{DynamicResource SystemAccentColorDark1}"/>
		<StaticResourceExtension x:Key="CheckBoxCheckBackgroundFillCheckedDisabled" ResourceKey="ButtonBackgroundDisabled"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundFillCheckedPointerOver" Color="{DynamicResource SystemAccentColorLight1}"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundFillCheckedPressed" Color="{DynamicResource SystemAccentColorDark1}"/>
		<StaticResourceExtension x:Key="CheckBoxCheckBackgroundFillIndeterminateDisabled" ResourceKey="ButtonBackgroundDisabled"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundFillIndeterminatePointerOver" Color="{DynamicResource SystemAccentColorLight1}"/>
		<SolidColorBrush x:Key="CheckBoxCheckBackgroundFillIndeterminatePressed" Color="{DynamicResource SystemAccentColorDark1}"/>
		<StaticResourceExtension x:Key="CheckBoxCheckBackgroundFillUncheckedDisabled" ResourceKey="ButtonBackgroundDisabled"/>

		<!-- ComboBox -->
		<StaticResourceExtension x:Key="ComboBoxBackground" ResourceKey="ButtonBackground"/>
		<StaticResourceExtension x:Key="ComboBoxBackgroundDisabled" ResourceKey="ButtonBackgroundDisabled"/>
		<StaticResourceExtension x:Key="ComboBoxBackgroundPointerOver" ResourceKey="ButtonBackgroundPointerOver"/>
		<StaticResourceExtension x:Key="ComboBoxBackgroundPressed" ResourceKey="ButtonBackgroundPressed"/>
		<StaticResourceExtension x:Key="ComboBoxBorderBrush" ResourceKey="ButtonBorderBrush"/>
		<StaticResourceExtension x:Key="ComboBoxBorderBrushDisabled" ResourceKey="ButtonBorderBrushDisabled"/>
		<StaticResourceExtension x:Key="ComboBoxBorderBrushPointerOver" ResourceKey="ButtonBorderBrush"/>
		<StaticResourceExtension x:Key="ComboBoxBorderBrushPressed" ResourceKey="ButtonBorderBrushPressed"/>
		<SolidColorBrush x:Key="ComboBoxDropDownBorderBrush" Color="{DynamicResource Color/Control.Border}"/>
		<Thickness x:Key="ComboBoxDropdownBorderPadding">4,3,4,3</Thickness>
		<Thickness x:Key="ComboBoxDropdownBorderThickness">1,2,1,2</Thickness>
		<Thickness x:Key="ComboBoxDropdownContentMargin">0</Thickness>

		<!-- ComboBoxItem -->
		<CornerRadius x:Key="CornerRadius/ComboBoxItem">5</CornerRadius>
		<sys:Double x:Key="Double/ComboBoxItem.Icon.Size">16</sys:Double>
		<Thickness x:Key="Thickness/ComboBoxItem.Icon.Margin">0,0,10,0</Thickness>
		<Thickness x:Key="Thickness/ComboBoxItem.Margin">0,1,0,1</Thickness>
		<Thickness x:Key="Thickness/ComboBoxItem.Padding">6,6,15,6</Thickness>

		<!-- Control -->
		<SolidColorBrush x:Key="Brush/Control.Border" Color="{DynamicResource Color/Control.Border}"/>
		<SolidColorBrush x:Key="Brush/Control.Border.Dark" Color="{DynamicResource Color/Control.Border.Dark}"/>
		<SolidColorBrush x:Key="Brush/Control.Border.Light" Color="{DynamicResource Color/Control.Border.Light}"/>
		<Color x:Key="Color/Control.Border">#000000</Color>
		<Color x:Key="Color/Control.Border.Dark">#000000</Color>
		<Color x:Key="Color/Control.Border.Light">#000000</Color>
		<CornerRadius x:Key="ControlCornerRadius">4</CornerRadius>
		<CornerRadius x:Key="ControlCornerRadiusBottom">0,0,4,4</CornerRadius>
		<CornerRadius x:Key="ControlCornerRadiusLeft">4,0,0,4</CornerRadius>
		<CornerRadius x:Key="ControlCornerRadiusRight">0,4,4,0</CornerRadius>
		<CornerRadius x:Key="ControlCornerRadiusTop">4,4,0,0</CornerRadius>
		<Thickness x:Key="Thickness/Control.Border">1</Thickness>
		<Thickness x:Key="Thickness/Control.Border.Focused">2</Thickness>

		<!-- DatePicker -->
		<StaticResourceExtension x:Key="DatePickerButtonBackground" ResourceKey="ButtonBackground"/>
		<StaticResourceExtension x:Key="DatePickerButtonBackgroundDisabled" ResourceKey="ButtonBackgroundDisabled"/>
		<StaticResourceExtension x:Key="DatePickerButtonBackgroundPointerOver" ResourceKey="ButtonBackgroundPointerOver"/>
		<StaticResourceExtension x:Key="DatePickerButtonBackgroundPressed" ResourceKey="ButtonBackgroundPressed"/>
		<StaticResourceExtension x:Key="DatePickerButtonBorderBrush" ResourceKey="ButtonBorderBrush"/>
		<StaticResourceExtension x:Key="DatePickerButtonBorderBrushDisabled" ResourceKey="ButtonBorderBrushDisabled"/>
		<StaticResourceExtension x:Key="DatePickerButtonBorderBrushPointerOver" ResourceKey="ButtonBorderBrush"/>
		<StaticResourceExtension x:Key="DatePickerButtonBorderBrushPressed" ResourceKey="ButtonBorderBrushPressed"/>

		<!-- Dialog -->
		<SolidColorBrush x:Key="Brush/Dialog.ControlButtonsPanel.Background" Color="{DynamicResource Color/Dialog.ControlButtonsPanel.Background}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Background.Header1" Color="{DynamicResource Color/Dialog.TextBlock.Background.Header1}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Background.Header2" Color="{DynamicResource Color/Dialog.TextBlock.Background.Header2}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Border.Header1" Color="{DynamicResource Color/Dialog.TextBlock.Border.Header1}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Border.Header2" Color="{DynamicResource Color/Dialog.TextBlock.Border.Header2}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Foreground.Description" Color="{DynamicResource Color/Dialog.TextBlock.Foreground.Description}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Foreground.Error" Color="{DynamicResource Color/Dialog.TextBlock.Foreground.Error}"/>
		<SolidColorBrush x:Key="Brush/Dialog.TextBlock.Foreground.Warning" Color="{DynamicResource Color/Dialog.TextBlock.Foreground.Warning}"/>
		<Color x:Key="Color/Dialog.ControlButtonsPanel.Background">#ffffff</Color>
		<Color x:Key="Color/Dialog.TextBlock.Background.Header1">#000000</Color>
		<Color x:Key="Color/Dialog.TextBlock.Background.Header2">#000000</Color>
		<Color x:Key="Color/Dialog.TextBlock.Border.Header1">#ffffff</Color>
		<Color x:Key="Color/Dialog.TextBlock.Border.Header2">#ffffff</Color>
		<Color x:Key="Color/Dialog.TextBlock.Foreground.Description">#000000</Color>
		<Color x:Key="Color/Dialog.TextBlock.Foreground.Error">#ff0000</Color>
		<Color x:Key="Color/Dialog.TextBlock.Foreground.Warning">#ff6a00</Color>
		<CornerRadius x:Key="CornerRadius/Dialog.TextBlock.Header1">5</CornerRadius>
		<CornerRadius x:Key="CornerRadius/Dialog.TextBlock.Header2">5</CornerRadius>
		<sys:Double x:Key="Double/Dialog.ControlButton.MinWidth">100</sys:Double>
		<sys:Double x:Key="Double/Dialog.ControlButton.Separator.Width">15</sys:Double>
		<sys:Double x:Key="Double/Dialog.Icon.Size">16</sys:Double>
		<sys:Double x:Key="Double/Dialog.Icon.Size.Large">40</sys:Double>
		<sys:Double x:Key="Double/Dialog.IconControlButton.Icon.Size">14</sys:Double>
		<sys:Double x:Key="Double/Dialog.ListActionButton.MinWidth">80</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Height">15</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Height.Large">40</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Height.Micro">3</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Height.Small">5</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Width">15</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Width.Large">40</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Width.Micro">3</sys:Double>
		<sys:Double x:Key="Double/Dialog.Separator.Width.Small">5</sys:Double>
		<sys:Double x:Key="Double/Dialog.TextBlock.FontSize">14</sys:Double>
		<sys:Double x:Key="Double/Dialog.TextBlock.FontSize.Description">13</sys:Double>
		<sys:Double x:Key="Double/Dialog.TextBlock.FontSize.Header1">15</sys:Double>
		<sys:Double x:Key="Double/Dialog.TextBlock.FontSize.Header2">14</sys:Double>
		<Thickness x:Key="Thickness/Dialog.ContentPanel.Margin.Indented">28,0,0,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.ContentPanel.Padding">20,15,20,15</Thickness>
		<Thickness x:Key="Thickness/Dialog.ContentPanel.Padding.WithHeader">30,15,30,15</Thickness>
		<Thickness x:Key="Thickness/Dialog.ControlButton.Margin">3,0,3,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.ControlButton.Padding">10,5,10,5</Thickness>
		<Thickness x:Key="Thickness/Dialog.ControlButtonsPanel.Padding">15</Thickness>
		<Thickness x:Key="Thickness/Dialog.Icon.Margin">2,0,14,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.Icon.Margin.Large">2,0,20,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.IconControlButton.Padding">8</Thickness>
		<Thickness x:Key="Thickness/Dialog.ListActionButton.Padding">10,5,10,5</Thickness>
		<Thickness x:Key="Thickness/Dialog.ListActionButton.Margin">5,5,0,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Border.Header1">1</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Border.Header2">1</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Padding.Header1">20,10,20,10</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Padding.Header2">20,5,20,5</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Margin">0,0,0,5</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Margin.Description">0,5,0,0</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Margin.Header1">-20,0,-20,13</Thickness>
		<Thickness x:Key="Thickness/Dialog.TextBlock.Margin.Header2">-20,0,-20,10</Thickness>

		<!-- Expander -->
		<SolidColorBrush x:Key="ExpanderBackgroundDisabled" Color="{DynamicResource SystemAltMediumHighColor}" Opacity="0.5"/>
		<SolidColorBrush x:Key="ExpanderDropDownBackground" Color="{DynamicResource ExpanderDropDownBackground}" Opacity="0.5"/>
		<StaticResourceExtension x:Key="ExpanderBorderBrush" ResourceKey="Brush/Control.Border"/>
		<Thickness x:Key="ExpanderDropdownDownBorderThickness">0</Thickness>
		<Thickness x:Key="ExpanderDropdownLeftBorderThickness">0</Thickness>
		<Thickness x:Key="ExpanderDropdownRightBorderThickness">0</Thickness>
		<Thickness x:Key="ExpanderDropdownUpBorderThickness">0</Thickness>

		<!-- ExternalDependenciesDialog -->
		<sys:Double x:Key="Double/ExternalDependenciesDialog.Content.MaxHeight">500</sys:Double>
		<sys:Double x:Key="Double/ExternalDependenciesDialog.Width">500</sys:Double>

		<!-- FullWindowTutorialPresenter -->
		<Color x:Key="Color/FullWindowTutorialPresenter.Background">#cf000000</Color>
		<BoxShadows x:Key="BoxShadows/FullWindowTutorialPresenter.Tutorial">0 0 10 0 #5f000000</BoxShadows>
		<SolidColorBrush x:Key="Brush/FullWindowTutorialPresenter.Background" Color="{DynamicResource Color/FullWindowTutorialPresenter.Background}"/>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.Description.FontSize">14</sys:Double>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.Description.MaxWidth">250</sys:Double>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.Dismiss.Size">24</sys:Double>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.Icon.Size">20</sys:Double>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.Offset">20</sys:Double>
		<sys:Double x:Key="Double/FullWindowTutorialPresenter.Tutorial.SkipAllTutorials.FontSize">13</sys:Double>
		<Thickness x:Key="Thickness/FullWindowTutorialPresenter.Icon.Margin">0,0,15,0</Thickness>
		<Thickness x:Key="Thickness/FullWindowTutorialPresenter.Dismiss.Margin">15,0,0,0</Thickness>
		<Thickness x:Key="Thickness/FullWindowTutorialPresenter.Dismiss.Padding">5</Thickness>
		<Thickness x:Key="Thickness/FullWindowTutorialPresenter.SkipAllTutorials.Margin">5,15,0,0</Thickness>
		<Thickness x:Key="Thickness/FullWindowTutorialPresenter.Tutorial.Padding">15</Thickness>

		<!-- GridSplitter -->
		<sys:Double x:Key="Double/GridSplitter.Thickness">5</sys:Double>

		<!-- Icon -->
		<Color x:Key="Color/Icon">#000000</Color>
		<Color x:Key="Color/Icon.Dark">#000000</Color>
		<Color x:Key="Color/Icon.Error">#ff3232</Color>
		<Color x:Key="Color/Icon.Information">#288acc</Color>
		<Color x:Key="Color/Icon.Light">#ffffff</Color>
		<Color x:Key="Color/Icon.Lightbulb">#ffd800</Color>
		<Color x:Key="Color/Icon.OK">#59b259</Color>
		<Color x:Key="Color/Icon.Question">#009999</Color>
		<Color x:Key="Color/Icon.Warning">#ff8432</Color>
		<SolidColorBrush x:Key="Brush/Icon" Color="{DynamicResource Color/Icon}"/>
		<SolidColorBrush x:Key="Brush/Icon.Active" Color="{DynamicResource Color/Accent}"/>
		<SolidColorBrush x:Key="Brush/Icon.Dark" Color="{DynamicResource Color/Icon.Dark}"/>
		<SolidColorBrush x:Key="Brush/Icon.Error" Color="{DynamicResource Color/Icon.Error}"/>
		<SolidColorBrush x:Key="Brush/Icon.Information" Color="{DynamicResource Color/Icon.Information}"/>
		<SolidColorBrush x:Key="Brush/Icon.Light" Color="{DynamicResource Color/Icon.Light}"/>
		<SolidColorBrush x:Key="Brush/Icon.Lightbulb" Color="{DynamicResource Color/Icon.Lightbulb}"/>
		<SolidColorBrush x:Key="Brush/Icon.OK" Color="{DynamicResource Color/Icon.OK}"/>
		<SolidColorBrush x:Key="Brush/Icon.Question" Color="{DynamicResource Color/Icon.Question}"/>
		<SolidColorBrush x:Key="Brush/Icon.Warning" Color="{DynamicResource Color/Icon.Warning}"/>
		<sys:Double x:Key="Double/Icon.Opacity.Disabled">0.3</sys:Double>

		<!-- InPlaceAction -->
		<SolidColorBrush x:Key="Brush/InPlaceAction.Background.Checked" Color="{DynamicResource Color/InPlaceAction.Background.Checked}"/>
		<SolidColorBrush x:Key="Brush/InPlaceAction.Background.PointerOver" Color="{DynamicResource Color/InPlaceAction.Background.PointerOver}"/>
		<SolidColorBrush x:Key="Brush/InPlaceAction.Background.Pressed" Color="{DynamicResource Color/InPlaceAction.Background.Pressed}"/>
		<Color x:Key="Color/InPlaceAction.Background.Checked">#20000000</Color>
		<Color x:Key="Color/InPlaceAction.Background.PointerOver">#30ffffff</Color>
		<Color x:Key="Color/InPlaceAction.Background.Pressed">#30000000</Color>
		<sys:Double x:Key="Double/InPlaceAction.IconSize">10</sys:Double>
		<sys:Double x:Key="Double/InPlaceAction.IconSize.Large">16</sys:Double>
		<sys:Double x:Key="Double/InPlaceAction.IconSize.Medium">13</sys:Double>
		<sys:Double x:Key="Double/InPlaceAction.ReservedHeight">18</sys:Double>
		<sys:Double x:Key="Double/InPlaceAction.ReservedHeight.Large">24</sys:Double>
		<sys:Double x:Key="Double/InPlaceAction.ReservedHeight.Medium">21</sys:Double>
		<Thickness x:Key="Thickness/InPlaceAction.Margin">3,0,0,0</Thickness>
		<Thickness x:Key="Thickness/InPlaceAction.Padding">3</Thickness>
		<Thickness x:Key="Thickness/InPlaceAction.Padding.Large">5</Thickness>
		<Thickness x:Key="Thickness/InPlaceAction.Padding.Medium">4</Thickness>

		<!-- InputAssistancePopup -->
		<sys:Double x:Key="Double/InputAssistancePopup.Content.MaxHeight">250</sys:Double>
		<Thickness x:Key="Thickness/InputAssistancePopup.Padding">0</Thickness>

		<!-- ListBox -->
		<Thickness x:Key="Thickness/ListBox.Padding">3</Thickness>

		<!-- ListBoxItem -->
		<CornerRadius x:Key="CornerRadius/ListBoxItem">4</CornerRadius>
		<sys:Double x:Key="Double/ComboBoxItem.Icon.Size">16</sys:Double>
		<Thickness x:Key="Thickness/ListBoxItem.Icon.Margin">0,0,9,0</Thickness>
		<Thickness x:Key="Thickness/ListBoxItem.Margin">0,1,0,1</Thickness>
		<Thickness x:Key="Thickness/ListBoxItem.Padding">6,6,15,6</Thickness>

		<!-- MainWindow -->
		<sys:TimeSpan x:Key="TimeSpan/MainWindow.ContentPaddingTransition">0:0:0.8</sys:TimeSpan>

		<!-- Menu -->
		<Color x:Key="Color/Menu.Background">#ffffff</Color>
		<SolidColorBrush x:Key="Brush/Menu.Background" Color="{DynamicResource Color/Menu.Background}"/>
		<Thickness x:Key="Thickness/Menu.MenuItem.Padding">10,5,10,5</Thickness>
		<Thickness x:Key="Thickness/Menu.Padding">5,3,5,3</Thickness>

