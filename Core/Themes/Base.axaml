		<!-- MenuItem -->
		<CornerRadius x:Key="CornerRadius/MenuItem">5</CornerRadius>
		<sys:Double x:Key="Double/MenuItem.Icon.Size">16</sys:Double>
		<Thickness x:Key="Thickness/MenuItem.Padding">12,8,12,8</Thickness>

